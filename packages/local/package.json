{
  "name": "@paykit-sdk/local",
  "version": "1.1.0",
  "description": "PayKit Local Provider for development and testing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/browser.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/browser.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@paykit-sdk/core": "1.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@paykit-sdk/core": "workspace:*"
  }
}
